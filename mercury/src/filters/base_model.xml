<?xml version="1.0" encoding="UTF-8"?>
<!-- The filter is considered to match if the root element matches. -->
<!-- Tag names are filters themselves, denoting that the respective element's type must be the same as the tag. -->
<!-- "all" means that all subfilters needs to match. -->
<!-- Things like "any", "all", etc. have different semantic meanings for different tags. -->
<dict filter="all">
    <named-field name="modelSpecs">
        <dict filter="all">
            <named-field name="metadata">
                <dict>
                    <named-field name="name">
                        <!-- "any" means that the content can be anything (the type still needs to match the tag, though). -->
                        <string filter="any"/>
                    </named-field>
                    <named-field name="class">
                        <string filter="any"/>
                    </named-field>
                    <named-field name="description">
                        <string filter="any"/>
                    </named-field>
                </dict>
            </named-field>
            <named-field name="capabilities">
                <list filter="any"></list>
            </named-field>
            <named-field name="callSpecs">
                <dict filter="all">
                    <named-field name="signatureType">
                        <string filter="any"/>
                    </named-field>
                    <named-field name="input">
                        <dict filter="all">
                            <named-field name="type">
                                <type-identifier filter="any"/>
                            </named-field>
                            <named-field name="description">
                                <string filter="any"/>
                            </named-field>
                        </dict>
                    </named-field>
                    <named-field name="output">
                        <dict filter="all">
                            <named-field name="type">
                                <type-identifier filter="any"/>
                            </named-field>
                            <named-field name="description">
                                <string filter="any"/>
                            </named-field>
                        </dict>
                    </named-field>
                </dict>
            </named-field>
            <named-field name="properties">
                <dict filter="any"/>
            </named-field>
        </dict>
    </named-field>
    <named-field name="implementations">
        <dict filter="any"/>
    </named-field>
</dict>